<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="com.deepanshu.ContactList.Controller"
          xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10">
    <padding>
        <Insets left="10.0" bottom="10.0" top="10.0" right="10.0"/>
    </padding>
    <Label text="Address Book" GridPane.rowIndex="0" GridPane.columnIndex="0">
    </Label>
    <TableView fx:id="tableView" GridPane.rowIndex="1" GridPane.columnIndex="0">
        <columns>
            <TableColumn text="First Name">
            </TableColumn>
            <TableColumn text="Last Name">
            </TableColumn>
            <TableColumn text="Phone Number">
            </TableColumn>
            <TableColumn text="Email">
            </TableColumn>
        </columns>
    </TableView>
</GridPane>